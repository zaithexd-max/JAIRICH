Num1 = int(input("Enter first number: "));
Sign = input("Enter sign: ");
Num2 = int(input("Enter second number: "));
if Sign == '+':
    Result = Num1 + Num2;
    print ("\n\n\nOutput:",Result);
elif Sign == '-':
    Result = Num1 - Num2;
    print ("\n\n\nOutput:",Result);
elif Sign == '*':
    Result = Num1*Num2;
    print ("\n\n\nOutput:",Result);
elif Sign == '/':
    Result = Num1/Num2;
    print ("\n\n\nOutput:",Result);
